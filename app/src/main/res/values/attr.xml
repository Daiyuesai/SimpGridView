<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SimpleGridView">
        <attr name="boundSpace" format="dimension|reference"/>
        <attr name="middleSpace" format="dimension|reference"/>
        <attr name="column" format="integer"/>
    </declare-styleable>

    <declare-styleable name="FlowLayoutView_Layout">
        <attr name="flow_gravity" >
            <enum name="top" value="48"/>
            <enum name="center" value="17"/>
            <enum name="bottom" value="80"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="FlowLayoutView">
        <attr name="childDefultGravity" >
            <enum name="top" value="48"/>
            <enum name="center" value="17"/>
            <enum name="bottom" value="80"/>
        </attr>
        <attr name="layoutGravity">
            <enum name="left" value="3"/>
            <enum name="center" value="17"/>
            <enum name="right" value="5"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="NineLayoutView">

        <attr name="singleMaxSize" format="dimension|reference"/>
        <attr name="nine_middleSpace" format="dimension|reference"/>
    </declare-styleable>


    <declare-styleable name="InvariantImageView">
        <attr name="limitMode" >
            <enum value="1" name="limitModeWidth"/>
            <enum value="2" name="limitModeHeight"/>
        </attr>
    </declare-styleable>

</resources>